<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:Sample.ViewModels"
             xmlns:maui="clr-namespace:EzShell.Maui;assembly=EzShell.Maui"
             x:DataType="viewModels:TabPageTwoViewModel"
             maui:EzShellPageExtensions.ViewModelType="{x:Type viewModels:TabPageTwoViewModel}"
             x:Class="Sample.ContentPages.TabPageTwo">
    <ContentPage.Content>
        <Label 
            Margin="30"
            FontSize="42"
            VerticalOptions="Center"
            HorizontalOptions="Center"
            Text="{Binding ParameterText}"/>
    </ContentPage.Content>
</ContentPage>