<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:EZShellSample"
    x:Class="EZShellSample.ThirdDeepPage">
    <ContentPage.BindingContext>
        <vm:ThirdDeepViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Third Deep Page!" FontSize="42" HorizontalOptions="Center"/>
            <Label Text="{Binding Value}" HorizontalOptions="Center"/>
            <Button Text="Pop To Root w/ Parameter" Command="{Binding PopCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
