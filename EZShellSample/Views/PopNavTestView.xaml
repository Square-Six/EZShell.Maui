<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:EZShellSample"
    x:Class="EZShellSample.PopNavTestView">
    <ContentPage.BindingContext>
        <vm:PopNavTestViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Pop Nav Test Page!" FontSize="42" HorizontalOptions="Center"/>
            <Button Text="Go Back" Command="{Binding GoBackCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
